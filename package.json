    "test:target-array-fix": "npm run validate-format && echo 'âœ… Format validation passed. Full test suite requires Jest.'",
    "validate-real-case": "node ./scripts/validate-real-case.js",
    "diagnose-validation": "node ./scripts/diagnose-db-validation.js",
    "test:respect-structure": "node ./scripts/test-db-adapter-fix.js",
    "test:content-calendar": "tsx src/app/api/agents/copywriter/content-calendar/tests/run-test.ts",
    "test:content-extraction": "tsx src/app/api/agents/copywriter/content-calendar/tests/quick-test.ts",
    "test:calendar-jest": "jest --verbose src/app/api/agents/copywriter/content-calendar/__tests__",
    "test:chat-message": "jest --verbose src/tests/api/agents-chat-message.test.ts"
