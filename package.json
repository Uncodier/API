  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -H 0.0.0.0 -p 3001",
    "ws": "node wsServer.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run ws\"",
    "build": "next build",
    "postbuild": "node ./node_modules/.bin/pagefind --site .next/server/app --output-path public/_pagefind",
    "start": "next start -p 3001",
